{
	"printWidth": 120, // 一行的字符数,如果超过会进行换行,默认为80
	"tabWidth": 2, // 一个tab代表几个空格数
	"useTabs": false, // 是否使用tab进行缩进,默认为false,表示用空格进行缩减
	"semi": true, // 行位是否使用分号,默认为true
	"singleQuote": true, // 字符串是否使用单引号
	"quoteProps": "as-needed", // 对象的 key 仅在必要时用引号
	"jsxSingleQuote": true, // jsx 不使用单引号,而使用双引号
	"trailingComma": "none", // 是否使用尾逗号
	"bracketSpacing": true, // 在对象,数组括号与文字之间加空格
	"bracketSameLine": false, // 强制在标签后自动换行
	"arrowParens": "always", // 单参数箭头函数参数周围使用圆括号
	"vueIndentScriptAndStyle": false, // 不对 vue 文件中的 script 和 style 标签缩进
	"htmlWhitespaceSensitivity": "strict", // 对 HTML 的空白敏感度
	"endOfLine": "lf", // 结尾是 \n \r \n\r auto
	"jsxBracketSameLine": false, // jsx 标签闭合位置
	"proseWrap": "always" // 是否自动换行
}
